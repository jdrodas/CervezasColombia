    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="CervezasColombia.sqlite" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="CervezasColombia.sqlite">
